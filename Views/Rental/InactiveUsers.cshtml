@using Rentalmandu.Areas.Identity.Data
@model List<HajurKoCarRentalUser>

<h1>Inactive  Customer for long time 3 months</h1>

@if (Model.Count == 0)
{
    <p>No inactive users found.</p>
}
else
{
    <table class="table">
        <thead>
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var user in Model)
            {
                <tr>
                    <td>@user.FirstName</td>
                    <td>@user.LastName</td>
                    <td>@user.Email</td>
                </tr>
            }
        </tbody>
    </table>
}
