@model List<Rentalmandu.Models.CarRentalInfo>

@{
    ViewData["Title"] = "Frequently Not Rented Cars";
}

<h1>Frequently Not Rented Cars</h1>

@if (Model.Count == 0)
{
    <p>No Results Found.</p>
}
else
{
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>Car Name</th>
                <th>Rental Count</th>
                <th>Price Per Day</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var car in Model)
            {
                <tr>
                    <td>@car.CarInfo.CarName</td>
                    <td>@car.RentalCount</td>
                    <td>@car.CarInfo.PricePerDay</td>
                </tr>
            }
        </tbody>
    </table>
}